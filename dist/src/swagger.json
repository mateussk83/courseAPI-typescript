                    "responses": {
                        "200": {
                            "description": "Success"
                        },
                        "400": {
                            "description": "Email or password incorrect!"
                        }
                    }
                }
            }
        },
        "/users/avatar": {
            "patch": {
                "tags": ["Users"],
                "summary": "Insert avatar img ",
                "description": "insert avatar img in User",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "avatar": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            },
                            "Bearer": {
                                "securitySchemes": {
                                    "bearerAuth": {
                                        "type": "http",
                                        "scheme": "bearer",
                                        "bearerFormat": "JWT",
                                        "description": "JWT Authorization header using the Bearer Scheme."
                                    }
                                }
                            }
                        },
                        "responses": {
                            "204": {
                                "description": "No Content"
                            },
                            "401": {
                                "description": "Unauthorized"
                            }
